  "dependencies": {
    "@aws-sdk/client-athena": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "serverless": "^3.0.0",
    "uvu": "^0.5.6"
  }
}
